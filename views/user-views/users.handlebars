<h1>Users</h1>
<p>
  <a href="/users/new"><button type="button" class="btn btn-primary">Create User</button></a>
</p>
<table class="table table-striped">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Profile Link</th>
  </tr>
  {{#each users as |user|}}
  <tr>
    <td>{{user.fname}}</td>
    <td>{{user.lname}}</td>
    <td>{{user.email}}</td>
    <td>
      <a href="/users/{{user.id}}"><button type="button" class="btn btn-primary">VIEW</button></a>&nbsp;&nbsp;&nbsp;    
      {{> delete-user userId=user.id}}
    </td>
  </tr>
  {{/each}}

</table>